		"scripts": {
				"build:css": "postcss src/styles/index.scss -o src/styles/index.css",
				"watch:css": "postcss src/styles/index.scss -o src/styles/index.css -w",
				"devserver": "react-scripts start",
				"build-sw": "node ./src/sw-build.js",
				"clean-cra-sw": "rimraf -f build/precache-manifest.*.js && rimraf -f build/service-worker.js",
				"build": "react-scripts build && npm run build-sw && npm run clean-cra-sw",
				"test": "react-scripts test --env=jsdom",
				"eject": "react-scripts eject",
				"start": "node server/index.js",
				"nodemon": "nodemon server/index.js",
				"lint": "eslint --fix src/**/*.js src/**/*.jsx"
		},
		"eslintConfig": {
				"extends": "react-app"
		},
		"browserslist": {
				"production": [
						">0.2%",
						"not dead",
						"not op_mini all"
